{
    "name": "dazeroit/laravel-theme",
    "description": ":Laravel Theme Management",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Maruca",
            "email": "ivan.maruca@gmail.com",
            "homepage": "https://github.com/dazeroit/laravel-theme"
        }
    ],
    "homepage": "https://github.com/dazeroit/laravel-theme",
    "keywords": ["Laravel", "Theme"],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.5",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dazeroit\\Theme\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dazeroit\\Theme\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dazeroit\\Theme\\ThemeServiceProvider"
            ],
            "aliases": {
                "Theme": "Dazeroit\\Theme\\Facades\\Theme"
            }
        }
    }
}
